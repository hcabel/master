{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "cpp",
	"patterns": [
		{
			"include": "#strings"
		}
	],
	"repository": {
		"strings": {
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.other.placeholder",
					"match": "\\{([^\\{]*:[^\\}]*)?\\}"
				}
			]
		}
	},
	"scopeName": "source.cpp"
}